    <!-- http://www.amrita.org/terms#TestDocument -->

    <owl:NamedIndividual rdf:about="http://www.amrita.org/terms#TestDocument">
        <rdf:type rdf:resource="http://purl.org/ontology/bibo/AcademicArticle"/>
        <rdf:type rdf:resource="http://purl.org/ontology/bibo/Journal"/>
        <dc:publisher rdf:resource="http://www.amrita.org/terms#North-Holland"/>
